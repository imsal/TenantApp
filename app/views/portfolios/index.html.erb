<h3>List of Portfolios</h3>



<% @portfolio.each do |portfolio| %>
<table>
<thead>
<tr>
  <th>ID</th>
  <th>Name</th>
  <th>Team</th>
  <th>Last Updated</th>
</tr>
</thead>


<tr>
  <td><%= portfolio.id %></td>
  <td><%= portfolio.name %></td>
  <td><%= portfolio.team %></td>
  <td><%= portfolio.updated_at %></td>
</tr>


  <tr><td colspan="4"><strong>Properties</strong></td></tr>

  <% portfolio.properties.each do |x| %>
  <table>
    <td><%= x.id %></td>
    <td><%= x.name %></td>
    <td><%= x.entity %></td>
    <td><%= x.sq_ft %></td>
  </table>
  <% end %>




<% end %>


</table> <!-- END Table -->